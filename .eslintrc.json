{
  "extends": "next/core-web-vitals",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "semi": "off",
    "indent": "off",
    "react/display-name": "off",
    "react/no-unescaped-entities": "off",
    "react-hooks/exhaustive-deps": "off",
    "quotes": ["error", "double", { "allowTemplateLiterals": true }]
  }
}
